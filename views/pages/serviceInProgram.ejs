<%- include('../partials/header') %>
<main class="main" id="main">
    <div class="container">
        <section class="section" id="section">
            <div class="row row-cols-1 row-cols-md-3 gd-4">
                <% if(services.length){ %>
                    <% services.forEach(service => { %>
                        <div class="col" id="div">
                            <div class="card h-100">
                                <h2 class="card-body streaming-name">
                                    <%= service.streaming_platform %>
                                </h2>
                            </div>
                        </div>
                  <%  }) %>


               <% } else{ %>
                <p class="streaming-unavailable">Not Available for Streaming</p>

              <% } %>


        </section>

        </div>
    </div>
</main>
<%- include('../partials/footer') %>